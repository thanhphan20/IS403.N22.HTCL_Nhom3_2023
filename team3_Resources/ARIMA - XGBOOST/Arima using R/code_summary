setwd("C:/Users/tuanl/Downloads/DA/data")

library(readr)

data_month <- read_csv("data_month.csv", 
    col_types = cols(year = col_number(), 
        month = col_number(), `GDP grow rate` = col_skip(), 
        agriculture = col_skip(), industry = col_skip(), 
        service = col_skip(), `GDP real` = col_skip(), 
        `GDP per capital` = col_skip(), CPI = col_skip(), 
        `FDI in` = col_skip(), `FDI out` = col_skip(), 
        `inflation rate` = col_skip(), `exchange rate` = col_skip(), 
        exchange = col_skip(), export = col_skip(), 
        import = col_skip(), workers = col_skip(), 
        `unemployment rate` = col_double()))


data_month$year_month <- paste(data_month$year, data_month$month, sep = "-")

data_month <- data_month[complete.cases(data_month), ]

data_month <- subset(data_month, select = -c(year, month))

column_to_move <- "year_month"

data_month <- data_month[, c(column_to_move, setdiff(names(data_month), column_to_move))]

ur <- data_month

install.packages("fBasics")

library(tseries)

adf.test(ur$`unemployment rate`)